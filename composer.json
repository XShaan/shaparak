{
    "name": "asanpay/shaparak",
    "description": "Iranian payment gateways handler for laravel based applications",
    "keywords": [
        "payment",
        "shaparak",
        "shetab",
        "asanpay",
        "bank",
        "online payment",
        "gateway",
        "iran"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aboozar Ghaffari <Tartan>",
            "email": "aboozar.ghf@gmail.com"
        },
        {
            "name": "Milad Kianmehr",
            "email": "milad.kian@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-xml": "*",
        "ext-bcmath": "*",
        "illuminate/support": ">=8.0",
        "illuminate/view": ">=8.0",
        "samuraee/easycurl": "^1.0",
        "tartan/laravel-xlog": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Asanpay\\Shaparak\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Asanpay\\Shaparak\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
